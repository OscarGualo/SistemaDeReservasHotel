<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="462.0" prefWidth="678.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.sistemareservas.Controlador.VentanaReserva">
   <children>
      <HBox layoutX="69.0" prefHeight="58.0" prefWidth="546.0" style="-fx-background-color: #000000;">
         <children>
            <ImageView fitHeight="58.0" fitWidth="91.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@hotel.jpg" />
               </image>
            </ImageView>
            <Button fx:id="btInicio" mnemonicParsing="false" onMouseClicked="#mostrarInicio" prefHeight="150.0" prefWidth="176.0" style="-fx-background-color: #000000; -fx-alignment: center; -fx-font-size: 18; -fx-border-color: White;" text="Inicio" textFill="WHITE">
               <font>
                  <Font name="Cascadia Mono Regular" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btReservas" mnemonicParsing="false" onMouseClicked="#realizarReserva" prefHeight="67.0" prefWidth="150.0" style="-fx-background-color: #000000; -fx-alignment: center; -fx-border-color: white; -fx-font-size: 18;" text="Reservas" textFill="WHITE">
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <Button fx:id="btSalir" mnemonicParsing="false" onMouseClicked="#salirSesion" prefHeight="64.0" prefWidth="148.0" style="-fx-background-color: #000000; -fx-border-color: white; -fx-font-size: 18;" text="Salir" textFill="WHITE" />
         </children>
      </HBox>
      <AnchorPane layoutY="62.0" prefHeight="484.0" prefWidth="713.0" style="-fx-background-color: #6A5ACD;">
         <children>
            <TextArea fx:id="txtPrueba" layoutX="14.0" prefHeight="478.0" prefWidth="695.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
